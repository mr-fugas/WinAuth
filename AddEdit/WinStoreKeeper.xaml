<Window x:Class="AddEdit.WinStoreKeeper"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AddEdit"
        mc:Ignorable="d"
        Title="WinStoreKeeper" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Text="Окно кладовщика" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="40"/>
        <TextBlock Name="userName" Text="" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="40"/>
        <Button Grid.Row="2" Content="Назад" Name="btn_esc" Click="btn_esc_Click"/>
    </Grid>
</Window>
